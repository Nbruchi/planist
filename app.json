{
    "expo": {
        "name": "planist",
        "slug": "planist",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "planist",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.brucheins.planist"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/todoist-logo.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ],
            [
                "expo-dynamic-app-icon",
                {
                    "default": {
                        "image": "./assets/images/icon.png",
                        "prerendered": true
                    },
                    "dark": {
                        "image": "./assets/images/icon-dark.png",
                        "prerendered": true
                    },
                    "green": {
                        "image": "./assets/images/icon-green.png",
                        "prerendered": true
                    },
                    "blue": {
                        "image": "./assets/images/icon-blue.png",
                        "prerendered": true
                    }
                }
            ],
            "expo-sqlite",
            "expo-dynamic-app-icon",
            [
                "expo-dev-launcher",
                {
                    "launchMode": "most-recent"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "2aa7387f-7e2a-4841-949e-3bdd5c4aff98"
            }
        }
    }
}
